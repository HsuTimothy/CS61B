package jump61;

/** A helper class for minmax method
in AI.
@author Michael Ferrin*/
class Move {

    /** Constructor for the move class.
    @param index is explained below
    @param value is explained below*/
    Move(int index, int value) {
        _index = index;
        _value = value;
    }

    /** Index realitave to the game board.
    @return index number*/
    int index() {
        return _index;
    }

    /** Value given by staticEval.
    @return value of staticEval*/
    int value() {
        return _value;
    }

    /** Index representation. */
    private int _index;

    /** Value representation. */
    private int _value;
}
